<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<GridPane xmlns:fx="http://javafx.com/fxml" fx:id="root" stylesheets="@../css/style.css"
          fx:controller="com.nocmok.opengl.curve.controller.MainSceneController">

    <AnchorPane fx:id="demoContainer" GridPane.columnIndex="0" GridPane.rowIndex="0">

    </AnchorPane>

    <HBox fx:id="demos" spacing="5" alignment="CENTER_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="1">
        <padding>
            <Insets left="20" right="20" top="20" bottom="20">

            </Insets>
        </padding>
    </HBox>

    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" halignment="CENTER"/>
    </columnConstraints>

    <rowConstraints>
        <RowConstraints vgrow="ALWAYS" valignment="CENTER"/>
        <RowConstraints vgrow="NEVER" minHeight="50" valignment="CENTER"/>
    </rowConstraints>
</GridPane>